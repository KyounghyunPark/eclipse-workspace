<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

                   
<mapper namespace="etri.student">
	<insert id="insertRow" parameterType="student">
	insert into student(name, tel, age) values (#{name}, #{tel}, #{age})
	</insert>
	
	<delete id="deleteRow parameterType="student">
	delete from student where name = #{name}
	</delete>

	<select id="selectAll" resultType="student">
	select * from student
	</select>
</mapper>